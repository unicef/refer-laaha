{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 8 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "acquia/blt": "^12.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/access_unpublished": "^1.3",
        "drupal/acquia_connector": "^3.0",
        "drupal/acquia_purge": "^1.2",
        "drupal/acquia_search": "^3.0.0",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/amp": "^3.5",
        "drupal/amptheme": "^3.0",
        "drupal/autologout": "^1.3",
        "drupal/autosave_form": "^1.2",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/blazy": "^2.5",
        "drupal/block_inactive_users": "^1.6",
        "drupal/bootstrap": "^3.23",
        "drupal/ckeditor_tippy": "^1.0",
        "drupal/clientside_validation": "^3.0@RC",
        "drupal/coder": "^8.3.9",
        "drupal/color_field": "^2.5",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^1.7",
        "drupal/console": "^1.9",
        "drupal/content_lock": "^2.2",
        "drupal/core": "^9",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/core-vendor-hardening": "^9",
        "drupal/core_views_facets": "^2.0",
        "drupal/default_content": "^2.0@alpha",
        "drupal/default_content_extra": "^1.0@beta",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/domain": "1.x-dev@dev",
        "drupal/domain_access_search_api": "^1.0",
        "drupal/domain_entity": "^1.0@beta",
        "drupal/domain_lang": "1.x-dev@dev",
        "drupal/domain_language_negotiation": "3.0.x-dev@dev",
        "drupal/dropzonejs": "^2.5",
        "drupal/email_tfa": "^1.0",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_pdf": "^2.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/environment_indicator": "^4.0",
        "drupal/field_encrypt": "^3.0",
        "drupal/field_group": "^3.2",
        "drupal/google_analytics": "^3.1",
        "drupal/google_tag": "^1.4",
        "drupal/hacked": "^2.0@beta",
        "drupal/hook_event_dispatcher": "^3.0",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/imageapi_optimize_gd": "^2.0@beta",
        "drupal/imageapi_optimize_resmushit": "^2.0@beta",
        "drupal/imageapi_optimize_tinypng": "^1.1@beta",
        "drupal/lang_dropdown": "^2.0@beta",
        "drupal/layout_paragraphs": "^1.0",
        "drupal/mail_login": "^2.0",
        "drupal/menu_item_fields": "^1.5",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/menu_per_role": "^1.4",
        "drupal/menus_attribute": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/multiple_fields_remove_button": "^1.0@alpha",
        "drupal/mysql56": "^1.0@beta",
        "drupal/node_title_validation": "^1.22",
        "drupal/node_view_permissions": "^1.5",
        "drupal/nodeviewcount": "^1.0@alpha",
        "drupal/opigno_scorm": "^3.0",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_ee": "^1.8",
        "drupal/paragraphs_features": "^1.12",
        "drupal/paragraphs_modal_edit": "^1.0",
        "drupal/password_policy": "^3.1",
        "drupal/pathauto": "^1.8",
        "drupal/permissions_filter": "^1.2",
        "drupal/public_preview": "^1.0@beta",
        "drupal/pwa": "^1.5",
        "drupal/quick_node_clone": "^1.14",
        "drupal/robotstxt": "^1.4",
        "drupal/role_hierarchy": "^2.2",
        "drupal/search_api_autocomplete": "^1.5",
        "drupal/seckit": "^2.0",
        "drupal/session_limit": "^2.0@beta",
        "drupal/shield": "^1.4",
        "drupal/simple_menu_icons": "^2.3",
        "drupal/simple_sitemap": "^3.11",
        "drupal/site_audit": "^3.0@RC",
        "drupal/status_messages": "^4.2",
        "drupal/taxonomy_term_depth": "^2.5",
        "drupal/tour_ui": "^1.0@beta",
        "drupal/twig_tweak": "^3.1",
        "drupal/update_helper": "^2.0",
        "drupal/verf": "^1.0",
        "drupal/video_embed_field": "^2.4",
        "drupal/view_password": "^6.0",
        "drupal/views_attach_library": "^2.0",
        "drupal/views_exposed_filter_blocks": "^1.1",
        "drupal/views_infinite_scroll": "^1.9",
        "drupal/warmer": "^2.0",
        "drush/drush": "^10.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "drupal/coder": "^8.3",
        "acquia/blt": "^12.8",
        "overtrue/phplint": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/console-extend-plugin": true,
            "acquia/blt": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./docroot/"
            },
            "file-mapping": {
                "[web-root]/.htaccess": false,
                "[web-root]/robots.txt": false
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library"
            ],
            "docroot/profiles/unicef/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/unicef/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/profiles/unicef/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/unicef/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/mail_login": {
                "Mail": "patches/mail.patch"
            },
            "drupal/amp": {
                "amp theme issue": "https://www.drupal.org/files/issues/2021-03-18/amp-crash_core-3186185-12.patch",
                "big pipe": "https://www.drupal.org/files/issues/2019-02-20/amp-bigpipe-2723627-25.patch"
            },
            "drupal/domain_language_negotiation": {
                "lang issue": "patches/active_lang.patch"
            },
            "drupal/paragraphs": {
                "Fix for translation handler": "https://www.drupal.org/files/issues/2021-01-22/paragraph-entity-type-did-not-specify-translation-halder-3031598-45.patch",
                "No link canonical": "https://www.drupal.org/files/issues/2021-10-18/3238515-8-paragraphs-entity-toUrl-error.patch"
            },
            "drupal/tour_ui": {
                "tour class selector issue": "https://www.drupal.org/files/issues/2021-05-28/tour_ui-3214357-7.patch"
            },
            "drupal/core": {
                "Default moderation state for translated entity": "https://www.drupal.org/files/issues/2021-12-04/3150294-79.patch",
                "Fix for unpublished translations not visible": "https://www.drupal.org/files/issues/2020-06-09/3088790-10.patch",
                "Fix ajax Uncaught Type Error": "https://www.drupal.org/files/issues/2018-06-27/drupal-uninitialized_variable_url-2922677-8.patch"
            },
            "drupal/quick_node_clone": {
                "translation issue": "https://www.drupal.org/files/issues/2019-10-01/quick_node_clone-cloning_multilanguage_content_moderation-2975704-10.patch",
                "para issue": "patches/clone_paragraph.patch"
            },
            "drupal/autosave_form": {
                "author name issue": "patches/autosave.patch"
            },
            "drupal/ckeditor_tippy": {
                "popper js issue": "https://www.drupal.org/files/issues/2021-11-01/3247089-2.patch"
            },
            "drupal/content_lock": {
                "ajax modal popup": "https://www.drupal.org/files/issues/2020-08-25/3167103-content_lock_with_paragraphs_edit-2.patch"
            },
            "drupal/better_exposed_filters_jquery":{
                "jquery ui library":"https://www.drupal.org/files/issues/2021-11-17/jquery-touch-punch_3159222_73.patch"
            },
            "drupal/email_tfa": {
                "Athentication form": "patches/email_tfa_issue.patch"
            }    
        }
    }
}
